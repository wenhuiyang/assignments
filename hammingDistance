class Solution {
    public int hammingDistance(int x, int y) {
        //convert both to binary
        
        int binx = converttobinary(x);
        int biny = converttobinary(y);
        if (x >= y)  {
            for (int i = 0; i < x; i++) {
                
            }            
            
        }
        
    }
    
    
    int converttobinary (int num){
         while (num != 0) {
            m = m << 1;
            num = num >> 1;
        }
    }
}
